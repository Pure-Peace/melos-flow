{
  "name": "@melosstudio/flow-sdk",
  "version": "1.0.4",
  "description": "The flow (cadence) contract of Melos NFT and Marketplace. Contains sdk, scripts and tests.",
  "main": "dist/cjs",
  "module": "dist/esm",
  "files": [
    "dist"
  ],
  "scripts": {
    "do": "yarn generate-code && node ./_scripts.js",
    "build": "yarn generate-code && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --project tsconfig-build.json --module es2015 --target es2015 --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig-build.json --module commonjs --target es2015 --outDir dist/cjs",
    "clean": "rimraf ./build ./dist ./sdk-code",
    "test": "yarn generate-code && yarn test-no-gen",
    "test-no-gen": "jest --runInBand",
    "verify": "tsc --noEmit",
    "generate-code": "yarn clean && node ./generateCadence.js && node ./generateSdkCode.js",
    "contract-deploy:testnet": "flow deploy project --network=testnet --config-path ./flow.testnet.json",
    "contract-deploy:mainnet": "flow deploy project --network=mainnet --config-path ./flow.mainnet.json",
    "contract-update:testnet": "flow deploy project --network=testnet --update --config-path ./flow.testnet.json",
    "contract-update:mainnet": "flow deploy project --network=mainnet --update --config-path ./flow.mainnet.json",
    "buildPublish": "yarn build && yarn publish"
  },
  "author": "Pure-Peace",
  "license": "ISC",
  "dependencies": {
    "@onflow/fcl": "^0.0.78",
    "@onflow/types": "0.0.6",
    "elliptic": "^6.5.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.14",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "cross-env": "^7.0.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "flow-js-testing": "^0.2.3-alpha.6",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org"
  }
}